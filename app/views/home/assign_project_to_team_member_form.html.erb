<%= form_for :assign_project_team_member, url: assign_project_team_member_submit_path do |f| %>

  <div class="field">
    <%= f.label :user %>
    <select name="user">
        <%@team.users.each do |u|%>
           <option value="<%=u.id%>"><%=u.email%></option>
        <%end%>
    </select>
  </div>
  
  <div class="field">
    <%= f.label :project %>
    <select name="project">
        <%@team.projects.each do |p|%>
           <option value="<%=p.id%>"><%=p.name%></option>
        <%end%>
    </select>
  </div>
  
  <p>
    <%= f.submit %>
  </p>
<% end %>