<%= form_for :assign_project, url: assign_project_submit_path do |f| %>


  <div class="field">
    <%= f.label :team %>
    <select name="team" class="form-control" id="exampleInputEmail1" aria_describedby ="emailHelp">
        <%@teams.each do |t|%>
           <option value="<%=t.id%>"><%=t.name%></option>
        <%end%>
    </select>
  </div>

  <div class="field">
    <%= f.label :project %>
    <select name="project" class="form-control" id="exampleInputEmail1" aria_describedby ="emailHelp">
        <%@projects.each do |p|%>
           <option value="<%=p.id%>"><%=p.name%></option>
        <%end%>
    </select>
  </div>
  
  <p>
    <%= f.submit 'Assign', class: "btn btn-primary"%>
  </p>
<% end %>