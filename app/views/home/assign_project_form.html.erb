<%= form_for :assign_project, url: assign_project_submit_path do |f| %>


  <div class="field">
    <%= f.label :team %>
    <select name="team">
        <%@teams.each do |t|%>
           <option value="<%=t.id%>"><%=t.name%></option>
        <%end%>
    </select>
  </div>

  <div class="field">
    <%= f.label :project %>
    <select name="project">
        <%@projects.each do |p|%>
           <option value="<%=p.id%>"><%=p.name%></option>
        <%end%>
    </select>
  </div>
  
  <p>
    <%= f.submit %>
  </p>
<% end %>